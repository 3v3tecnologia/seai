{
  "name": "backend",
  "version": "1.0.0",
  "description": "Seai backend",
  "main": "dist/index.js",
  "author": "",
  "scripts": {
    "clean": "rm -rf ./dist",
    "test": "jest",
    "lint": "tslint --project tsconfig.json",
    "build": "tsc -p .",
    "build:debug": "npm run clean && npm run build -- --sourceMap",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "tsc -w",
    "build:start": "npm run build:clean && npm run start:prod",
    "db:seeds": "docker container exec -w /usr/src seai-database-dev bash seed.sh",
    "dev:watch": "concurrently -s all \"tsc -w\" nodemon dist/src/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/src/index.js",
    "start:dev:local": "tsx --env-file=.env.development.local --watch src/index.ts",
    "start:debug:container": "node --env-file=.env.development.local --import tsx --inspect=0.0.0.0:9229 --watch ./src/index.ts",
    "start:debug:local": "node --env-file=.env.development.local --import tsx --inspect=0.0.0.0:9229 --watch ./src/index.ts",
    "compose:up:dev": "docker compose -f docker-compose.dev.yml up -d ",
    "compose:down:dev": "docker compose -f docker-compose.dev.yml down",
    "container:logs": "docker container logs --tail 200 --timestamps seai-server",
    "container:logs:watch": "docker container logs -f --tail 100  seai-server",
    "knex:government": "dotenv -e ./.env -- knex --knexfile src/shared/infra/database/postgres/configs/government.ts",
    "knex:logs": "DOTENV_CONFIG_PATH=.env knex --knexfile src/shared/infra/database/postgres/configs/logs.ts",
    "knex:census": "DOTENV_CONFIG_PATH=.env knex --knexfile src/shared/infra/database/postgres/configs/census.ts"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.4.2",
    "@types/nodemailer": "^6.4.8",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "@types/validator": "^13.7.17",
    "concurrently": "^8.2.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tsx": "^4.15.6",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.8",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "knex-postgis": "^0.14.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.3",
    "pg": "^8.11.1",
    "pg-query-stream": "^4.6.0",
    "pino": "^9.1.0",
    "pino-pretty": "^11.1.0",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.0"
  }
}
